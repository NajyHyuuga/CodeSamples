            Map(x => x.LastCruiseBookDate).Name("Last Cruise booking date");
            Map(x => x.LastCruiseReturnDate).Name("Last Cruise Return Date");
            Map(x => x.LastResortBookDate).Name("Last Resort booking date");
            Map(x => x.LastResortReturnDate).Name("Last Resort Return Date");
            Map(x => x.LastCarBookDate).Name("Last Car booking date");
            Map(x => x.LastCarReturnDate).Name("Last Car Return Date");
            Map(x => x.LastAirBookDate).Name("Last Air booking date");
            Map(x => x.LastAirReturnDate).Name("Last Air Return Date");
            Map(x => x.LastWebVisitDate).Name("Last Web visit date");
            Map(x => x.UniqueVisitsPerDayLast90).Name("Unique Visits per day (Last 90)");
            Map(x => x.MostRecentHotelClick).Name("Most Recent Hotel Click");
            Map(x => x.MostRecentCruiseClick).Name("Most Recent Cruise Click");
