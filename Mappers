public sealed class Mappers: ClassMap<SorMember>
    {
        public Mappers()
        {
            Map(x => x.MemberId).Name("Account");
            Map(x => x.Address1).Name("Address 1");
            Map(x => x.Address2).Name("Address 2");
            Map(x => x.BActive).Name("B Active");
            Map(x => x.City).Name("City");
            Map(x => x.CountryId).Name("Country ID");
            Map(x => x.DoNotContact).Name("Do Not Contact");
            Map(x => x.EmailAddress).Name("Email Address");
            Map(x => x.FirstName).Name("First Name");
            Map(x => x.GroupId).Name("Group ID");
            Map(x => x.LastName).Name("Last Name");
            Map(x => x.Phone1).Name("Phone 1");
            Map(x => x.Phone1Dnc).Name("Phone 1 Do Not Contact");
            Map(x => x.CellPhone).Name("Phone 2");
            Map(x => x.Phone2Dnc).Name("Phone 2 Do Not Contact");
            Map(x => x.WorkPhone).Name("Phone 3");
            Map(x => x.Phone3Dnc).Name("Phone 3 Do Not Contact");
            Map(x => x.PostalCode).Name("Zip/Postal Code");
            Map(x => x.State).Name("State");
            Map(x => x.SecondaryFirstName).Name("Secondary First Name");
            Map(x => x.SecondaryLastName).Name("Secondary Last Name");
            Map(x => x.SecondaryEmail).Name("Secondary Email");
            Map(x => x.EnrollmentDate).Name("Enrollment Date");
            Map(x => x.ExpirationDate).Name("Expiration Date");
            Map(x => x.AccountStatus).Name("Account Status");
            Map(x => x.SourceSystem).Name("Source CRM");
            Map(x => x.PartnerName).Name("Partner Name");
            Map(x => x.SubPartner).Name("Sub Partner");
            Map(x => x.MemberClass).Name("Member Class");
            Map(x => x.TierLevel).Name("Tier Level");
            Map(x => x.LeadSourceName).Name("Lead Source");
            Map(x => x.VacationCashBalance).Name("Available Vacation Cash");
            Map(x => x.VacationCashExpiringNext60).Name("VC Expiring");
            Map(x => x.PointsBalance).Name("Available Points");
            Map(x => x.PointsExpiringNext60).Name("Points Expiring");
            Map(x => x.LastUpgradeDate).Name("Last Upgrade Date");
            Map(x => x.LastUpgradeBalance).Name("Balance Due");
            Map(x => x.FinalPaymentDate).Name("Final Payment Date");
            Map(x => x.TotalTravelSpend).Name("Total Travel Spend");
            Map(x => x.ElCidBookingAllowed).Name("ElCid Booking Allowed");
            Map(x => x.VidaBookingAllowed).Name("Vida Booking Allowed");
