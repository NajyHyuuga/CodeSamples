public sealed class Mappers: ClassMap<SorMember>
    {
        public Mappers()
        {
            Map(x => x.MemberId).Name("Account");
            Map(x => x.Address1).Name("Address 1");
            Map(x => x.Address2).Name("Address 2");
            Map(x => x.City).Name("City");
            //left it like this as an example. Mapped the rest of the fields as in the csv file.
        }
    }
